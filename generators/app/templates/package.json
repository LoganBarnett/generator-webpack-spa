    "extract-text-webpack-plugin": "^0.8.2",
    "fs": "0.0.2",
    "glob": "^5.0.14",
    "isparta-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^1.7.0",
