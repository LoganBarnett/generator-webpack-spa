{
  "name": "jolly.js",
  "version": "0.0.1",
  "description": "jolly.js is a project bootstrap with lots of default trimmings.",
  "repository": "git@github.com:LoganBarnett/jolly.js.git",
  "scripts": {
    "build": "mkdir -p ./dist && cp client/index.html dist/index.html && webpack client/app/index.js",
    "test": "karma start",
    "bs": "node browser-sync.js",
    "test-watch": "karma start --no-single-run karma.conf.js",
